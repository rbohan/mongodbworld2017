<?xml version='1.0' encoding='utf-8' ?>
<connection-customization class='mongodb' version='7.7' enabled='true'>
    <vendor name='mongodb' />
    <driver name='mongodb' />
    <customizations>
      <customization name='odbc-connect-string-extras' value='SSLKEY={/.../certs/mongo.key};SSLCERT={/.../certs/mongo.crt};SSLCA={/.../certs/ca.crt};ENABLE_CLEARTEXT_PLUGIN=1;SSL_ENFORCE=1' />
      </customizations>
</connection-customization>
